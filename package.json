{
  "name": "league-rage",
  "description": "League TDM gamemode",
  "version": "0.7a",
  "license": "ISC",
  "scripts": {
    "install": "yarn install:core && yarn install:tdm && yarn install:cef && yarn install:ui && yarn install:lang && yarn install:statistics && yarn install:mapeditor && yarn install:mapeditor-ui",
    "install:core": "yarn --cwd ./league-core",
    "install:tdm": "yarn --cwd ./league-tdm",
    "install:cef": "yarn --cwd ./league-tdm-cef",
    "install:ui": "yarn --cwd ./league-tdm-ui",
    "install:lang": "yarn --cwd ./league-lang",
    "install:statistics": "yarn --cwd ./league-tdm-statistics",
    "install:mapeditor": "yarn --cwd ./league-mapeditor",
    "install:mapeditor-ui": "yarn --cwd ./league-mapeditor-ui",
    "upgrade:all": "yarn upgrade:core && yarn upgrade:tdm && yarn upgrade:cef && yarn upgrade:ui && yarn upgrade:lang && yarn upgrade:statistics",
    "upgrade:core": "yarn upgrade --cwd ./league-core",
    "upgrade:tdm": "yarn upgrade --cwd ./league-tdm",
    "upgrade:cef": "yarn upgrade --cwd ./league-tdm-cef",
    "upgrade:ui": "yarn upgrade --cwd ./league-tdm-ui",
    "upgrade:lang": "yarn upgrade --cwd ./league-lang",
    "upgrade:statistics": "yarn upgrade --cwd ./league-tdm-statistics",
    "build": "yarn build:server && yarn build:client",
    "build:server": "yarn build:core && yarn build:lang && yarn build:tdm && yarn build:statistics && yarn build:mapeditor",
    "build:client": "yarn build:ui && yarn build:cef && yarn build:mapeditor-ui && yarn build:tdm-client",
    "build:core": "yarn --cwd ./league-core build:external",
    "build:tdm": "yarn --cwd ./league-tdm build:external",
    "build:ui": "yarn --cwd ./league-tdm-ui build:external",
    "build:cef": "yarn --cwd ./league-tdm-cef build:external",
    "build:lang": "yarn --cwd ./league-lang build:external",
    "build:tdm-client": "cp -R ./league-tdm-client/index.js ../client_packages/index.js",
    "build:statistics": "yarn --cwd ./league-tdm-statistics build:external",
    "build:mapeditor": "yarn --cwd ./league-mapeditor build:external",
    "build:mapeditor-ui": "yarn --cwd ./league-mapeditor-ui build:external",
    "source:server": "yarn source:core && yarn source:tdm && yarn source:lang",
    "source:core": "yarn --cwd ./league-core source:external",
    "source:tdm": "yarn --cwd ./league-tdm source:external",
    "source:lang": "yarn --cwd ./league-lang source:external",
    "source:statistics": "yarn --cwd ./league-tdm-statistics source:external",
    "watch": "concurrently --kill-others \"yarn watch:core\" \"yarn watch:tdm\" \"yarn watch:ui\" \"yarn watch:cef\" \"yarn watch:lang\" \"yarn watch:statistics\" \"yarn watch:mapeditor\" \"yarn watch:mapeditor-ui\"",
    "watch:server": "concurrently --kill-others \"yarn watch:core\" \"yarn watch:tdm\" \"yarn watch:lang\" \"yarn watch:statistics\" \"yarn watch:mapeditor\"",
    "watch:client": "concurrently --kill-others \"yarn watch:ui\" \"yarn watch:cef\" \"yarn watch:mapeditor-ui\"",
    "watch:core": "yarn --cwd ./league-core watch:external",
    "watch:tdm": "yarn --cwd ./league-tdm watch:external",
    "watch:ui": "yarn --cwd ./league-tdm-ui watch:external",
    "watch:cef": "yarn --cwd ./league-tdm-cef watch:external",
    "watch:cef:cache": "yarn --cwd ./league-tdm-cef watch:external:cache",
    "watch:lang": "yarn --cwd ./league-lang watch:external",
    "watch:statistics": "yarn --cwd ./league-tdm-statistics watch:external",
    "watch:mapeditor": "yarn --cwd ./league-mapeditor watch:external",
    "watch:mapeditor-ui": "yarn --cwd ./league-mapeditor-ui watch:external"
  },
  "dependencies": {},
  "devDependencies": {
    "@parcel/transformer-svg-react": "^2.8.3",
    "concurrently": "^7.6.0"
  }
}
